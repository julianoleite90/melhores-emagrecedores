{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "melhores-emagrecedores.com"
        }
      ],
      "destination": "https://www.melhores-emagrecedores.com",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "melhores-emagrecedores.com"
        }
      ],
      "destination": "https://www.melhores-emagrecedores.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
} 